{
    "name": "ministryofjustice/opg-lpa-front",
    "description": "Front end service for creating Lasting Power of Attorney PDF documents",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "framework",
        "zf2"
    ],
    "homepage": "http://framework.zend.com/",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ministryofjustice/opg-lpa-datamodels"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ministryofjustice/opg-lpa-logger"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.3",
        "aws/aws-sdk-php": "^3.0",
        "ministryofjustice/postcodeinfo-client-php": "^4.0",
        "ministryofjustice/opg-lpa-datamodels": "^10.0",
        "ministryofjustice/opg-lpa-logger": "^4.0",
        "michelf/php-markdown": "^1.8",
        "alphagov/pay-integration-php": "^1.0",
        "php-http/guzzle6-adapter": "^1.1",
        "soundasleep/html2text": "^0.5.0",
        "zendframework/zend-component-installer": "^1.1",
        "zendframework/zend-mvc": "^3.1",
        "zendframework/zend-authentication": "^2.5",
        "zendframework/zend-cache": "^2.7",
        "zendframework/zend-form": "^2.11",
        "zendframework/zend-session": "^2.8",
        "zendframework/zend-math": "^3.0",
        "zendframework/zend-crypt": "^3.2",
        "kokspflanze/zfc-twig": "^2.1",
        "zendframework/zend-mvc-i18n": "^1.0",
        "zendframework/zend-mail": "^2.8",
        "zendframework/zend-json": "^3.1",
        "zendframework/zend-console": "^2.7",
        "zendframework/zend-test": "^3.1",
        "sendgrid/sendgrid": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/"
        }
    }
}
