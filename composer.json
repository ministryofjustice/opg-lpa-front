        "zendframework/zendframework": "2.3.*",
        "sendgrid/sendgrid": "2.2.*",
        "ministryofjustice/opg-lpa-api-client": "dev-develop",
        "ministryofjustice/opg-lpa-datamodels": "dev-master",
        "Netsensia/omnipay-worldpay-xml": "*",
