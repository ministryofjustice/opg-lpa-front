{
    "name": "ministryofjustice/opg-lpa-front",
    "description": "Front end service for creating Lasting Power of Attorney PDF documents",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "framework",
        "zf2"
    ],
    "homepage": "http://framework.zend.com/",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ministryofjustice/opg-lpa-datamodels"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ministryofjustice/opg-lpa-logger"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Netsensia/omnipay-worldpay-xml"
        }
    ],
    "require": {
        "php": "7.0.*",
        "guzzlehttp/guzzle": "^5.1",
        "guzzlehttp/psr7" : "^1.2",
        "zendframework/zendframework": "^2.5",
        "sendgrid/sendgrid": "2.2.*",
        "aws/aws-sdk-php": "^3.0",
        "ministryofjustice/postcodeinfo-client-php": "^4.0",
        "ministryofjustice/opg-lpa-datamodels": "dev-LPA-2543-who-list",
        "ministryofjustice/opg-lpa-logger": "^2.0",
        "Netsensia/omnipay-worldpay-xml": "^1.0.1",
        "michelf/php-markdown": "1.5.0",
        "zf-commons/zfc-twig": "^1.2.2",
        "nsmithuk/zf-config-dump": "^1.0",
        "alphagov/pay-integration-php": "^1.0.0",
        "php-http/client-implementation": "^1.0",
        "php-http/guzzle5-adapter": "^1.0",
        "php-http/httplug": "^1.0",
        "php-http/message": "^1.2",
        "psr/http-message": "^1.0",
        "soundasleep/html2text": "^0.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "mockery/mockery": "^1.0"
    }
}
