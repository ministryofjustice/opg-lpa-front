{
	"name": "ministryofjustice/opg-lpa-api-client",
    "description": "API Wrapper for opg-lpa-api",
    "license": "MIT",
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/ministryofjustice/opg-lpa-datamodels"
      }
    ],
    "require": {
        "php": ">=5.5.9",
        "guzzlehttp/guzzle": "^5.1.0",
        "ministryofjustice/opg-lpa-datamodels": "4.0.*",
        "psr/http-message": "^1.0",
        "php-http/httplug": "^1.0",
        "guzzlehttp/psr7" : "^1.2",
        "php-http/client-implementation": "^1.0",
        "php-http/message": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "phpspec/phpspec": "~2.0",
        "php-http/message": "^1.1",
        "php-http/guzzle5-adapter": "*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {"Opg\\Lpa\\Api\\Client\\": "src/Opg/Lpa/Api/Client/"}
    }
}
