{
    "name": "kokspflanze/zfc-twig",
    "description": "Zend Framework Module that provides a Twig rendering strategy and extensions to render actions or trigger events from your templates",
    "type": "library",
    "keywords": [
        "twig",
        "module",
        "zf"
    ],
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/kokspflanze/ZfcTwig/",
    "authors": [
        {
            "name": "Kyle Spraggs",
            "email": "theman@spiffyjr.me",
            "homepage": "http://www.spiffyjr.me/"
        },
        {
            "name": "Cosmin Harangus",
            "email": "cosmin@zendexperts.com",
            "homepage": "http://www.zendexperts.com/"
        }
    ],
    "require": {
        "php": "^7.0",
        "twig/twig": "^2.4.2",
        "zendframework/zend-mvc": "^3.0",
        "zendframework/zend-view": "^2.7",
        "zendframework/zend-servicemanager": "^3.0",
        "zendframework/zend-mvc-plugin-flashmessenger": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ZfcTwig\\": "src/ZfcTwig"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZfcTwigTest\\": "tests/ZfcTwigTest"
        }
    },
    "require-dev": {
        "zendframework/zend-navigation": "^2.7",
        "zendframework/zend-modulemanager": "^2.7",
        "phpunit/phpunit": "^6.5 || ^7.0.3"
    },
    "replace": {
        "zf-commons/zfc-twig": "self.version"
    },
    "extra": {
        "zf": {
            "module": "ZfcTwig"
        },
        "branch-alias": {
            "dev-release/1.4": "1.4-dev",
            "dev-master": "2.2-dev",
            "dev-develop": "2.3-dev"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always --configuration tests/phpunit.xml"
    }
}
